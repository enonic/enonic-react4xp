    "Enonic",
    "XP",
    "React"
  ],
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "name": "@enonic/react4xp",
  "peerDependencies": {
    "@babel/plugin-proposal-class-properties": "^7",
    "react": "*",
    "react-dom": "*",
    "webpack": "^5",
    "webpack-cli": "^4"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:enonic/enonic-react4xp.git"
  },
  "scripts": {
    "build": "npx tsc",
    "build:components:test": "./bin/buildConstants.js test/buildComponents \"$(< test/buildComponents/overrides.json)\"",
    "build:nashornPolyfills:constants:test": "./bin/buildConstants.js test/nashornPolyfills \"$(< test/nashornPolyfills/overrides.json)\"",
    "build:nashornPolyfills:test": "webpack --config nashornPolyfills/webpack.config.js --env NASHORNPOLYFILLS_SOURCE=test/nashornPolyfills/testPolyfiller.es6 --env BUILD_ENV=development --env BUILD_R4X=test/nashornPolyfills/output",
    "clean": "rm -r *.tgz bin build dist externals/_AUTOGENERATED_tmp_externals_.es6 test/buildComponents/build test/buildComponents/dummy-build test/constants/build test/constants/output test/nashornPolyfills/output 2>/dev/null; true",
    "lint": "eslint src/**/*.es6 src/**/*.js",
    "prebuild": "npm run clean",
    "prepack": "npm run build",
    "pretest:components:raw": "npm run build:components:test",
    "pretest:components:override": "npm run build:components:test",
    "test": "npm run test:components && npm run test:constants && test:externals:webpack && npm run test:nashornPolyfills:constants",
    "test:components": "npm run test:components:raw && npm run test:components:override",
    "test:components:raw": "webpack --config dist/webpack.config.components.js --env ENTRY_DIRS=entries --env CHUNK_DIRS=sharedComps,redux --env ROOT=$(pwd)/../.. --env OVERRIDE_COMPONENT_WEBPACK=test/buildComponents/webpack.config.symlinks.override.js --env REACT4XP_CONFIG_FILE=test/buildComponents/build/constants.json --env VERBOSE=true",
    "test:components:override": "webpack --config dist/webpack.config.components.js --env OVERRIDE_COMPONENT_WEBPACK=test/buildComponents/webpack.config.override.js --env ROOT=$(pwd)/../.. --env VERBOSE=true --env REACT4XP_CONFIG_FILE=test/buildComponents/build/constants.json",
    "test:externals:webpack": "NODE_PATH=. webpack --config dist/webpack.config.externals.js --env REACT4XP_CONFIG_FILE=/test/externals/react4xpConstants.json --env BUILD_R4X=$(pwd)/build --env ROOT=$(pwd)/../.. && echo \"\" && echo \"--> build/\" && ls build && echo \"-------------- react4xp-runtime-externals builds. ADD SOME ACTUAL TESTS PLZ?\"",
    "test:nashornPolyfills:constants": "webpack --config nashornPolyfills/webpack.config.js --env REACT4XP_CONFIG_FILE=test/nashornPolyfills/output/testConfig.json && node test/nashornPolyfills/output/testPolyfilled.js",
    "tsc": "npx tsc",
    "webpack:components": "npx webpack --config dist/webpack.config.components.js",
    "webpack:externals": "npx webpack --config dist/webpack.config.externals.js",
    "webpack:nashornPolyfills": "npx webpack --config nashornPolyfills/webpack.config.js"
  },
  "version": "3.0.5"
}
