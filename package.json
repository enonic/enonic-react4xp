  },
  "files": [
    "buildComponents/README.md",
    "buildComponents/webpack.config.js",
    "dist/**/*.js",
    "externals/README.md",
    "externals/webpack.config.js",
    "nashornPolyfills/README.md",
    "nashornPolyfills/webpack.config.js",
    "src/**/*.ts",
    "LICENSE",
    "npmInstall.gradle",
    "react4xp.gradle",
    "react4xp.properties",
    "README.md",
    "updaters.gradle"
  ],
  "homepage": "https://github.com/enonic/enonic-react4xp#readme",
  "keywords": [
